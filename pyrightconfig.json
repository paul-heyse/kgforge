{
  "typeCheckingMode": "standard",
  "pythonVersion": "3.13",
  "pythonPlatform": "Linux",
  "venvPath": ".",
  "venv": ".venv",
  "useLibraryCodeForTypes": true,
  "stubPath": "stubs",
  "include": ["src", "codeintel_rev", "tools", "tests"],
  "exclude": [
    ".git",
    ".hg",
    ".venv",
    ".ruff_cache",
    ".pytest_cache",
    "build",
    "dist",
    "node_modules",
    "docs/_build/**",
    "site/**",
    "faiss/**",
    "src/kgfoundry.egg-info/**",
    "kgfoundry.egg-info/**",
    "src/kgfoundry_common/http/**",
    "codeintel_rev/xtr-warp/**",
    "codeintel_rev/patches/**",
    "docs/**"
  ],
  "executionEnvironments": [
    {
      "root": "src",
      "extraPaths": [
        "src",
        "codeintel_rev",
        "tools",
        "stubs",
        ".venv/lib/python3.13/site-packages"
      ],
      "pythonVersion": "3.13"
    },
    {
      "root": "codeintel_rev",
      "extraPaths": [
        "src",
        "codeintel_rev",
        "tools",
        "stubs",
        ".venv/lib/python3.13/site-packages"
      ],
      "pythonVersion": "3.13"
    },
    {
      "root": "tools",
      "extraPaths": [
        "src",
        "codeintel_rev",
        "tools",
        "stubs",
        ".venv/lib/python3.13/site-packages"
      ],
      "pythonVersion": "3.13"
    }
  ]
}
