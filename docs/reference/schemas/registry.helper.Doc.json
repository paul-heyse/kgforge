{
  "$defs": {
    "Id": {
      "type": "string"
    }
  },
  "$id": "https://kgfoundry/schemas/registry.helper.Doc.json#",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Model the Doc.\n\nPydantic model defining the structured payload used across the system. Validation ensures inputs\nconform to the declared schema while producing clear error messages. Use this class when\nserialising or parsing data for the surrounding feature.",
  "examples": [
    {
      "arxiv_id": {},
      "authors": null,
      "content_hash": {},
      "doi": {},
      "id": null,
      "language": "en",
      "license": {},
      "openalex_id": {},
      "pdf_uri": "",
      "pmcid": {},
      "pub_date": {},
      "source": "unknown",
      "title": ""
    }
  ],
  "properties": {
    "arxiv_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Arxiv Id"
    },
    "authors": {
      "items": {
        "type": "string"
      },
      "title": "Authors",
      "type": "array"
    },
    "content_hash": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Content Hash"
    },
    "doi": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Doi"
    },
    "id": {
      "$ref": "#/$defs/Id"
    },
    "language": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": "en",
      "title": "Language"
    },
    "license": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "License"
    },
    "openalex_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Openalex Id"
    },
    "pdf_uri": {
      "default": "",
      "title": "Pdf Uri",
      "type": "string"
    },
    "pmcid": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Pmcid"
    },
    "pub_date": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Pub Date"
    },
    "source": {
      "default": "unknown",
      "title": "Source",
      "type": "string"
    },
    "title": {
      "default": "",
      "title": "Title",
      "type": "string"
    }
  },
  "required": [
    "id"
  ],
  "title": "Doc",
  "type": "object"
}
