Module(body=[Expr(value=Constant(value='Filesystem utilities using pathlib for safe, typed operations.\n\nThis module provides helpers for directory management, file I/O, and path\noperations using `pathlib.Path` exclusively. All functions are fully typed\nand documented to replace legacy `os.path` usage.\n\nExamples\n--------\n>>> from pathlib import Path\n>>> from kgfoundry_common.fs import ensure_dir, read_text, write_text\n>>> base = Path("/tmp/test")\n>>> ensure_dir(base / "subdir")\n>>> write_text(base / "file.txt", "content")\n>>> content = read_text(base / "file.txt")\n', lineno=1, col_offset=0, end_lineno=15, end_col_offset=3), lineno=1, col_offset=0, end_lineno=15, end_col_offset=3), ImportFrom(module='__future__', names=[alias(name='annotations', lineno=19, col_offset=23, end_lineno=19, end_col_offset=34)], level=0, lineno=19, col_offset=0, end_lineno=19, end_col_offset=34), Import(names=[alias(name='sys', lineno=21, col_offset=7, end_lineno=21, end_col_offset=10)], lineno=21, col_offset=0, end_lineno=21, end_col_offset=10), Import(names=[alias(name='tempfile', lineno=22, col_offset=7, end_lineno=22, end_col_offset=15)], lineno=22, col_offset=0, end_lineno=22, end_col_offset=15), ImportFrom(module='pathlib', names=[alias(name='Path', lineno=23, col_offset=20, end_lineno=23, end_col_offset=24)], level=0, lineno=23, col_offset=0, end_lineno=23, end_col_offset=24), ImportFrom(module='typing', names=[alias(name='Literal', lineno=24, col_offset=19, end_lineno=24, end_col_offset=26)], level=0, lineno=24, col_offset=0, end_lineno=24, end_col_offset=26), ImportFrom(module='kgfoundry_common.logging', names=[alias(name='get_logger', lineno=26, col_offset=37, end_lineno=26, end_col_offset=47)], level=0, lineno=26, col_offset=0, end_lineno=26, end_col_offset=47), Assign(targets=[Name(id='logger', ctx=Store(), lineno=28, col_offset=0, end_lineno=28, end_col_offset=6)], value=Call(func=Name(id='get_logger', ctx=Load(), lineno=28, col_offset=9, end_lineno=28, end_col_of