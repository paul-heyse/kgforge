irOptions', body=[Expr(value=Constant(value='Configuration for navmap repair operations.\n\n    This configuration object controls how the navmap repair tool identifies\n    and optionally applies fixes to module navmap metadata.\n\n    Attributes\n    ----------\n    root : Path | None, optional\n        Root directory to scan for modules with navmap metadata.\n        Defaults to None (uses project src/ directory).\n    apply : bool, optional\n        When True, write fixes back to disk. When False, emit suggested changes\n        to stdout. Defaults to False.\n    emit_json : bool, optional\n        When True, emit machine-readable JSON results. When False, emit human-readable\n        text output. Defaults to False.\n\n    Examples\n    --------\n    >>> options = NavmapRepairOptions(apply=False, emit_json=True)\n    >>> options.apply\n    False\n    >>> options.emit_json\n    True\n    ', lineno=31, col_offset=4, end_lineno=55, end_col_offset=7), lineno=31, col_offset=4, end_lineno=55, end_col_offset=7), AnnAssign(target=Name(id='root', ctx=Store(), lineno=57, col_offset=4, end_lineno=57, end_col_offset=8), annotation=BinOp(left=Name(id='Path', ctx=Load(), lineno=57, col_offset=10, end_lineno=57, end_col_offset=14), op=BitOr(), right=Constant(value=None, lineno=57, col_offset=17, end_lineno=57, end_col_offset=21), lineno=57, col_offset=10, end_lineno=57, end_col_offset=21), value=Constant(value=None, lineno=57, col_offset=24, end_lineno=57, end_col_offset=28), simple=1, lineno=57, col_offset=4, end_lineno=57, end_col_offset=28), AnnAssign(target=Name(id='apply', ctx=Store(), lineno=58, col_offset=4, end_lineno=58, end_col_offset=9), annotation=Name(id='bool', ctx=Load(), lineno=58, col_offset=11, end_lineno=58, end_col_offset=15), value=Constant(value=False, lineno=58, col_offset=18, end_lineno=58, end_col_offset=23), simple=1, lineno=58, col_offset=4, end_lineno=58, end_col_offset=23), AnnAssign(target=Name(id='emit_json', ctx=Store(), lineno=59, col_offset=4,